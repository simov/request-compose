{
  "name": "request-compose",
  "version": "0.0.14",
  "description": "Composable HTTP Client",
  "keywords": [
    "compose",
    "composable",
    "http",
    "client"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/simov/request-compose",
  "author": "Simeon Velichkov <simeonvelichkov@gmail.com> (https://simov.github.io)",
  "repository": {
    "type": "git",
    "url": "https://github.com/simov/request-compose.git"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^5.0.0",
    "request-logs": "0.0.5",
    "request-multipart": "0.0.1",
    "request-oauth": "0.0.2"
  },
  "bin": {},
  "main": "./build/index.js",
  "files": [
    "build/request/",
    "build/response/",
    "build/utils/",
    "build/compose.js",
    "build/index.js",
    "request/",
    "response/",
    "utils/",
    "compose.js",
    "index.js",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "npm run test:ci",
    "test:ci": "v=$(node -e \"console.log(process.version.split('.')[0].slice(1))\") && if [ $v -ge 8 ]; then p=\"test\"; else p=\"build/test\"; fi && mocha $p --recursive",
    "test:cov": "istanbul cover _mocha -- --recursive",
    "build:request": "babel request --out-dir build/request",
    "build:response": "babel response --out-dir build/response",
    "build:utils": "babel utils --out-dir build/utils",
    "build:test": "babel test --out-dir build/test",
    "copy:test": "cp -r test/ssl/ build/test/ && cp -r test/fixtures/ build/test/",
    "build:main": "babel *.js --out-dir build",
    "build": "rm -rf build/ && mkdir -p build && npm run build:request && npm run build:response && npm run build:utils && npm run build:test && npm run copy:test && npm run build:main",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
